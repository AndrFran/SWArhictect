<Window x:Class="SWArchitecture.UserControls.StatsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SWArchitecture.UserControls"
        mc:Ignorable="d"
        Title="StatsWindow" Height="300" Width="300">
    <Grid>
        <DataGrid Name="Data" AutoGenerateColumns="false">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Mark" Binding="{Binding Mark}" />
                <DataGridTextColumn Header="Date" Binding="{Binding Date}" />
            </DataGrid.Columns>
        </DataGrid>

    </Grid>
</Window>
